{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "src",
    "strictNullChecks": true,
    "types": ["astro/client", "unplugin-icons/types/astro"],
    "paths": {
      "@lib/*": ["lib/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@assets/*": ["./src/assets/*"],
      "@pages/*": ["pages/*"]
    }
  }
}
